{
    "name": "thisisdevelopment/laravel-api-token-middleware",
    "type": "library",
    "keywords": [
        "laravel",
        "package",
        "plugin"
    ],
    "description": "Simple API token middleware for internal APIs",
    "autoload": {
        "psr-4": {
            "ThisIsDevelopment\\LaravelApiTokenMiddleware\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Sascha Mendel",
            "email": "sascha@thisisdevelopment.nl"
        }
    ],
    "require": {
        "php": ">=7",
        "laravel/framework": "^9.2|^10.0|^11.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ThisIsDevelopment\\LaravelApiTokenMiddleware\\ApiTokenServiceProvider"
            ]
        }
    }
}
